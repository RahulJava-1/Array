#include<stdio.h>
#include<conio.h>
int main(int argc, char const *argv[])
{
    int a[3][3],b[3][3],c[3][3],i,j;
    printf("Enter the first matrix element: ");
    for ( i = 0; i < 3; i++)
    {
        for ( j = 0; j < 3; j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    printf("\n");
    printf("Enter the second matrix element: ");
    for ( i = 0; i < 3; i++)
    {
        for ( j = 0; j < 3; j++)
        {
            scanf("%d",&b[i][j]);
        }
    }
    printf("\n printing output: \n");
    for ( i = 0; i < 3; i++)
    {
        for ( j = 0; j < 3; j++)
        {
            c[i][j] = a[i][j]+b[i][j];
            printf("%d ",c[i][j]);
        }
        printf("\n");
    }   
    return 0;
}
